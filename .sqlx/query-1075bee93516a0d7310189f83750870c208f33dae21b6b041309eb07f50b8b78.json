{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            m.id,\n            m.user_id as user_id,\n            m.message_id,\n            m.from_address,\n            m.to_addresses,\n            m.cc_addresses,\n            m.bcc_addresses,\n            m.subject,\n            m.body_text,\n            m.body_html,\n            m.in_reply_to,\n            m.thread_references,\n            f.name as folder,\n            m.is_read,\n            m.is_starred,\n            m.received_at,\n            m.created_at\n        FROM admin_email_messages m\n        JOIN admin_email_folders f ON m.folder_id = f.id\n        WHERE m.id = $1 AND m.user_id = $2\n        ",
